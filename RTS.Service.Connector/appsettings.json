{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ConnectorDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "System.Net.Http.HttpClient": "None",
      "System.Net.Http.HttpClient.Tracelink": "None",
      "System.Net.Http.HttpClient.Economic": "None"
    }
  },

  "AppSettings": {
    "AllowedOrigins": "*",
    "AllowedMethods": "GET, POST",
    "AllowedHeaders": "*"
  },

  "Economic": {
    "BaseUrl": "https://restapi.e-conomic.com/",
    "AgreementGrantToken": "",
    "AppSecretToken": "",
    "TimeoutSeconds": 30,
    "TestTotalAmount":  10000,
    "EndPoints": {
      "CreateDraft": "/invoices/drafts",
      "GetOrderDraft": "/orders/drafts/"
    }
  },

  "Tracelink": {
    "BaseUrl": "https://tracelink.app/rest/",
    "ApiToken": "",
    "TimeoutSeconds": 30,
    "RetryCount": 2,
    "EndPoints": {
      "GetOrderList": "/tracelink/order/list",
      "GetOrder": "/tracelink/order/",
      "GetCustomerList": "/object/list/module/customer",
      "GetCrmList": "/object/list/module/crm"
    }
  }
}
